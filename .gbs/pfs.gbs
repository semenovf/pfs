kind          "StaticLib"
language      "C++"
targetname    "pfs-mpl"
files         {
                  "../src/pfs/*.cpp"
                , "../src/pfs/io/*.cpp"
                , "../src/pfs/application/*.cpp"
              }
links         { PFS_LINKS }


configuration "debug"
    objdir       ".cache/pfs-mpl/debug"

configuration "release"
    objdir       ".cache/pfs-mpl/release"

filter "platforms:unix*"
    files {
              "../src/pfs/gnuc/*.cpp"
            , "../src/pfs/posix/*.cpp"
            , "../src/pfs/io/posix/*.cpp"
            , "../src/pfs/application/posix/*.cpp"
          }
